<%= link_to "Back to Pictures", pictures_url %>

<div class="form">

<h1>ADD A PICTURE</h1>

<div class="photo_icon">
  <%= image_tag ("photo_icon-03.svg")%>
</div>

<%= form_for @picture do |f| %>
  <p class="form_input">
    <%= f.label :artist %><br>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url %><br>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>

</div>
